{
    "version": 2,
    "alias": "sophie-tally-ho-app",
    "name": "Tally-ho!",
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "^/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": ".*",
            "dest": "/index.html"
        }
    ]
}